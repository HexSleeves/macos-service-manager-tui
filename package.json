{
	"name": "macos-service-manager",
	"version": "0.1.0",
	"description": "A Terminal UI for managing macOS LaunchDaemons, LaunchAgents, and System Extensions",
	"author": "HexSleeves",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/HexSleeves/macos-service-manager-tui.git"
	},
	"keywords": [
		"macos",
		"launchd",
		"launchctl",
		"services",
		"tui",
		"terminal",
		"cli"
	],
	"module": "src/index.tsx",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "bun run --watch src/index.tsx",
		"format": "bunx biome format --write",
		"lint": "bunx biome lint",
		"lint:fix": "bunx biome lint --write",
		"check": "bunx biome check",
		"check:fix": "bunx biome check --write",
		"typecheck": "bunx tsc --noEmit",
		"test": "bun test"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@types/bun": "^1.3.8"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@opentui/core": "^0.1.77",
		"@opentui/react": "^0.1.77",
		"@types/react": "^19.2.10",
		"react": "^19.2.4",
		"zustand": "^5.0.11"
	}
}
